{
    "name": "vardot/varbase-build",
    "description": "Drupal Bundled with Necessities | by Vardot",
    "type": "drupal-profile",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "drush/drush": "^9.0",
        "drupal/drupal-extension": "3.1.5",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.8",
        "drupal/devel": "^8.0.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.drupal-composer.org"
        }
    ],
    "extra": {
        "installer-paths": {
            "docroot/core": [
                "drupal/core"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ]
        }
    },
    "require": {
        "cweagans/composer-patches": "^1.5.0",
        "composer/installers": "^1.0",
        "drupal/core": "^8.1.0",
        "drupal/embed": "^8.1.0",
        "drupal/entity_embed": "8.1.0-alpha1",
        "drupal/media_entity": "^8.1.0",
        "drupal/media_entity_instagram": "^8.1.0",
        "drupal/media_entity_twitter": "^8.1.0",
        "drupal/media_entity_image": "^8.1.0",
        "drupal/ctools": "8.3.0-alpha26",
        "drupal/panels": "8.3.0-beta4",
        "drupal/page_manager": "8.1.0-alpha23",
        "drupal/panelizer": "8.3.0-alpha2",
        "drupal/scheduled_updates": "8.1.0-alpha5",
        "drupal/workbench_moderation": "8.1.0",
        "drupal/acquia_connector": "^8.1.1",
        "drupal/config_update": "^8.1.1",
        "drupal/features": "^8.3.0",
        "drupal/inline_entity_form": "^8.1.0",
        "drupal/metatag": "^8.1.0",
        "drupal/token": "^8.1.0",
        "drupal-composer/drupal-scaffold": "^2.0.0",
        "drupal/pathauto": "^8.1.0",
        "drupal/entity_browser": "8.1.0-alpha3",
        "drupal/views_infinite_scroll": "8.1.0",
        "drupal/media_entity_document": "^8.1.0",
        "drupal/video_embed_field": "^8.1.0"
    }
}
